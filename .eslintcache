oject/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/seunghwankim/Main/Main.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/taehyungkim/Login/Login.js",["75","76"],"import React, { Component } from 'react';\nimport { withRouter } from 'react-router-dom';\nimport { SIGNUP_API, SIGNIN_API } from '../data/config'\nimport './Login.scss'\n\n\nclass LoginTaeHyung extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      idValue : '',\n      pwValue : '',\n      isPwShown : false,\n      token: '',\n    }\n  }\n\n  handleInput = (e) => {\n    const { name, value } = e.target;  \n    this.setState({ [name]: value })\n  }\n\n  handleClick = () => {\n    const { isPwShown } = this.state;\n    this.setState({ isPwShown: !isPwShown })\n  }\n\n  checkToken = () => {\n    const token = localStorage.getItem('token');\n    console.log({ token })\n  }\n\n  checkValidation = (e) => {\n    e.preventDefault();\n    const { idValue, pwValue } = this.state;\n    const checkResult = (idValue.includes('@') && pwValue.length >= 8);\n    if (!checkResult) {\n      alert('올바르지 않은 이메일, 비밀번호 입니다.')\n      return;\n    }\n    fetch(SIGNIN_API, {\n      method: \"POST\",\n      body: JSON.stringify({\n        email: idValue,\n        password: pwValue,\n      })\n    })\n      .then((response) => response.json())\n      .then((result) => {\n        localStorage.setItem(\"token\", result.Authorization);\n        this.setState({\n          token: result.Authorization\n        })\n        if (result.message === \"SUCCESS\" && this.state.token) {\n          this.props.history.push('/main-taehyung');\n        }\n        else {\n          alert('가입되지 않은 계정입니다!');\n          this.props.history.push('/login-taehyung');\n          this.setState({\n            idValue: '',\n            pwValue: '',\n          })\n        }\n      })\n  }\n\n  render() {\n    const { idValue, pwValue, isPwShown, token } = this.state;\n    const { handleInput, handleClick, checkValidation, checkToken } = this;\n    const regex = /^.*(?=^.{8,15}$)(?=.*\\d)(?=.*[a-zA-Z])(?=.*[!@#$%^&+=]).*$/;\n    const activeBtn = (idValue.length && pwValue.length) !== 0;\n    \n    return (\n      <div className=\"Login\">\n        <main className=\"login-container\">\n          <h1>instargram</h1>\n          <form onSubmit={checkValidation}>\n            <input \n              type=\"text\"\n              name='idValue'\n              value={idValue}\n              onChange={(e) => handleInput(e)}\n              placeholder=\"전화번호, 사용자 이름 또는 이메일\" />\n            <p className={idValue && idValue.length < 6 && 'active'}>아이디는 최소 6글자 이상의 이메일 형식!</p>\n            <div className=\"input-pw-container\">\n              <input \n                type={isPwShown ? \"text\" : \"password\"}\n                name='pwValue'\n                value={pwValue}\n                onChange={(e) => handleInput(e)}\n                placeholder=\"비밀번호\" />\n              <span className=\"show-pw-btn\" onClick={handleClick}>\n                {isPwShown ? 'Hide' : 'Show'}\n              </span>\n              <p className={pwValue && !regex.test(pwValue) && 'active'}>\n                비밀번호는 8~15자의 영문(대소)/숫자/특수문자 혼합!\n              </p>              \n            </div>\n            <button type=\"submit\" className={`${activeBtn ? 'active' : ''}`}>\n                로그인\n            </button>\n            <button type=\"button\" onClick={checkToken}>토큰확인</button>\n          </form>\n          <p className=\"find-pw\"><a href=\"/password-finder\">비밀번호를 잊으셨나요?</a></p>\n        </main>\n      </div>\n    )\n  }\n}\n\nexport default withRouter(LoginTaeHyung);","/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/KiyeolKim/Login/Login.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/donghalee/Main/Main.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/KiyeolKim/Main/Main.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/taehyungkim/Main/Main.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/donghalee/Login/Login.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/taehyungkim/data/config.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/taehyungkim/data/data.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/taehyungkim/Main/Components/comment.js",[],["77","78"],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/taehyungkim/Main/Components/navbar.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/taehyungkim/Main/Components/aside.js",[],"/Users/taeng9/Desktop/project/wecode/3-4주차_foundation/16-React-Westagram-1/src/Pages/taehyungkim/Main/Components/feed.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":3,"column":10,"nodeType":"85","messageId":"86","endLine":3,"endColumn":20},{"ruleId":"83","severity":1,"message":"87","line":69,"column":42,"nodeType":"85","messageId":"86","endLine":69,"endColumn":47},{"ruleId":"79","replacedBy":"88"},{"ruleId":"81","replacedBy":"89"},"no-native-reassign",["90"],"no-negated-in-lhs",["91"],"no-unused-vars","'SIGNUP_API' is defined but never used.","Identifier","unusedVar","'token' is assigned a value but never used.",["90"],["91"],"no-global-assign","no-unsafe-negation"]